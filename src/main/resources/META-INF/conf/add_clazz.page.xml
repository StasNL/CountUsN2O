<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <form>
        <datasource query-id="clazz" object-id="clazz">
            <filters>
                <eq field-id="id" param="id"/>
            </filters>
        </datasource>
        <fields>
            <input-text id="study_year" label="Год обучения" required="true" domain="integer"
                        min="1" max="11"/>
            <input-text id="mnemocode" label="Мнемокод" required="true"/>
        </fields>
    </form>
</simple-page>
